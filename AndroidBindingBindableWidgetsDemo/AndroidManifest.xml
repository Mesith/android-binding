<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gueei.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="7" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:name="LayoutDemoApplication" >
        <activity
            android:label="@string/app_name"
            android:name=".MainActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainBindableFrameLayoutActivity"/>
        <activity android:name=".MainBindableLinearLayoutActivity"/>
        <activity android:name=".MainBindableTableLayoutActivity"/>        
		<activity android:name="com.gueei.demo.framelayout.SimpleDemoActivity"/>
		<activity android:name="com.gueei.demo.framelayout.WizardDemoActivity"/>
		<activity android:name="com.gueei.demo.framelayout.ConfigChangeDemoActivity"
			android:configChanges="orientation|keyboard"
		/>			
		<!-- android:configChanges="screenSize|orientation|keyboard" for 3.x -->
    </application>

</manifest>