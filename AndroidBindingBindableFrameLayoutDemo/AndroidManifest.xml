<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.gueei.demos.bindableframelayoutdemo"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="7" />

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name="LayoutDemoApplication">
        <activity android:name=".MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity android:name=".SimpleDemoActivity"/>
		<activity android:name=".WizardDemoActivity"/>
		<activity android:name=".ConfigChangeDemoActivity"
			android:configChanges="orientation|keyboard"
		/>			
		<!-- android:configChanges="screenSize|orientation|keyboard" for 3.x -->
    </application>
</manifest>